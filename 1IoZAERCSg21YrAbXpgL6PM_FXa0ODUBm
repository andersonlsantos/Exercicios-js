
// 32)​ Construir um algoritmo que calcule a média aritmética dos valores de um vetor de inteiros.

function media(...vetor){
    let cont = 0
    let acum = 0
    for(let i = 0; i < vetor.length; i++){
        cont += 1
        acum += vetor[i]
    }
    return acum / cont
}

console.log(media(1, 2, 3, 4, 5))
