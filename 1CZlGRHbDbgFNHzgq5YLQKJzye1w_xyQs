
/**
 * 01)​ Crie uma função que dado dois valores (passados como parâmetros) mostre no console a soma, subtração,
    multiplicação e divisão desses valores.
 */

function somaSutracapMultiplicacaoDivicao(a, b){
    console.log(`${a} + ${b} = ${a + b}`)
    console.log(`${a} - ${b} = ${a - b}`)
    console.log(`${a} * ${b} = ${a * b}`)
    console.log(`${a} / ${b} = ${a / b}`)
}

somaSutracapMultiplicacaoDivicao(45, 10)